<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.example.administrator.myapplication.MainActivity">

    <ProgressBar
        android:id="@+id/pb_load_progress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"/>

    <TextView
        android:text="Hello World!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_matrix"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="matrix"/>

    <Button
        android:id="@+id/btn_bitmap_cut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="bitmap cut"/>

    <Button
        android:id="@+id/btn_permission_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Permission test"/>

    <Button
        android:id="@+id/btn_leak"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Leak"/>

    <Button
        android:id="@+id/btn_leak2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Leak2"/>

    <Button
        android:id="@+id/btn_leak3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Leak3"/>


    <Button
        android:id="@+id/btn_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test rv"/>

    <Button
        android:id="@+id/btn_delay_start_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Delay start activity"/>


    <Button
        android:id="@+id/btn_test_rx_error_subscriber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test rx error subscriber"/>

    <Button
        android:id="@+id/btn_test_rx"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test rx"/>

    <Button
        android:id="@+id/btn_test_rx2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test rx2"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Leak2"/>

</LinearLayout>
